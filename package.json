{
  "name": "seim-service",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.6.0"
  },
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "doc": "npx compodoc -p tsconfig.json -n \"test Service documentation\" -s",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --logHeapUsage --runInBand",
    "test:watch": "jest --detectOpenHandles --logHeapUsage --watch",
    "test:cov": "jest --detectOpenHandles --coverage --logHeapUsage --runInBand",
    "test:debug": "node -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config jest.debug.ts --runInBand",
    "dbm:init": "cd migrations && ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js schema:sync -d ../ormconfig.migration.ts",
    "dbm:add": "cd migrations && ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js migration:generate -d ../ormconfig.migration.ts",
    "dbm:run": "cd migrations && ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js migration:run -d ../ormconfig.migration.ts",
    "dbm:revert": "cd migrations && ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js migration:revert -d ../ormconfig.migration.ts",
    "dbs:generate": "cd seeds && ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js migration:create -n -d ../ormconfig.seed.ts",
    "dbs:run": "cd seeds && ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js migration:run -d ../ormconfig.seed.ts",
    "dbs:revert": "cd seeds && ts-node -r tsconfig-paths/register ../node_modules/typeorm/cli.js migration:revert -d ../ormconfig.seed.ts",
    "libs:build": "meta git update && meta git checkout tags/v1.10.0 --include-only libs"
  },
  "dependencies": {
    "@nestjs/bull": "0.6.0",
    "@nestjs/common": "8.4.7",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "8.4.7",
    "@nestjs/cqrs": "8.0.5",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "8.4.7",
    "@nestjs/platform-express": "8.4.7",
    "@nestjs/swagger": "5.2.1",
    "@nestjs/terminus": "8.1.1",
    "@nestjs/typeorm": "8.1.4",
    "@types/swagger-ui-express": "4.1.3",
    "axios": "0.27.2",
    "bull": "4.8.5",
    "cache-manager": "4.1.0",
    "cache-manager-ioredis": "2.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.5",
    "dotenv": "16.0.1",
    "ejs": "3.1.8",
    "email-templates": "10.0.1",
    "ethers": "^6.13.4",
    "express": "4.18.1",
    "firebase-admin": "^12.0.0",
    "helmet": "6.0.0",
    "ioredis": "5.2.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "2.3.3",
    "nanoid": "3.3.4",
    "nest-winston": "1.7.0",
    "nestjs-i18n": "^10.5.0",
    "pluralize": "8.0.0",
    "qs": "6.11.0",
    "redis": "4.3.0",
    "referral-codes": "^2.0.3",
    "rimraf": "3.0.2",
    "rrule": "2.7.1",
    "rxjs": "7.5.6",
    "swagger-ui-express": "4.5.0",
    "typeorm": "0.3.7",
    "uuid": "8.3.2",
    "winston": "3.8.1",
    "ts-loader": "8.4.0",
    "winston-transport-sentry-node": "2.6.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.19",
    "@nestjs/cli": "8.2.8",
    "@nestjs/schematics": "8.0.11",
    "@types/bull": "3.15.9",
    "@types/cache-manager-ioredis": "2.0.2",
    "@types/compression": "1.7.2",
    "@types/crypto-js": "4.1.1",
    "@types/decimal.js": "7.4.0",
    "@types/email-templates": "10.0.0",
    "@types/express": "4.17.13",
    "@types/multer": "1.4.7",
    "@types/node": "14.17.5",
    "@types/pluralize": "0.0.29",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "3.4.1",
    "fast-glob": "3.2.11",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.2.3"
  },
  "lint-staged": {
    "*.ts": [
      "yarn run format",
      "yarn run lint"
    ]
  }
}
